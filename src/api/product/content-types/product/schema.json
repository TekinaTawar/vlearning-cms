{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "view": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "validity": {
      "type": "date",
      "required": true
    },
    "combo": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::combo.combo"
    },
    "course": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::subject.subject"
    },
    "format": {
      "type": "enumeration",
      "enum": [
        "USB",
        "Download"
      ]
    },
    "studyMaterial": {
      "type": "enumeration",
      "enum": [
        "Book",
        "Softcopy"
      ]
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "isCombo": {
      "type": "boolean",
      "required": true
    }
  }
}
